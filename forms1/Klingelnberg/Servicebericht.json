    "1-maschine": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "de": "Allgemeine Informationen",
        "en": "General Information"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "required": true,
      "children": {
        "asset": {
          "fieldType": "REMBERG_ASSET_SELECT",
          "label": {
            "de": "Maschine",
            "en": "Select Asset"
          },
          "required": true,
          "disableSelection": false,
          "hidePreselection": false,
          "showUnassigned": true,
          "editShowInput": true,
          "registerAssetChange": true,
          "width": 0.5,
          "messages": {
            "onChange": [
              {
                "to": "1-maschine",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": "asset",
                            "_propertyName": "product"
                          },
                          "_functionName": "GET_PRODUCT"
                        },
                        "_propertyName": "location"
                      },
                      "output": {
                        "_field": "customerDestination"
                      }
                    }
                  }
                ]
              },
              {
                "to": "1-maschine",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": "asset",
                              "_propertyName": "product"
                            },
                            "_functionName": "GET_PRODUCT"
                          },
                          "_propertyName": "customer"
                        },
                        "_functionName": "GET_ACCOUNT"
                      },
                      "output": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "reportId": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Service-Auftragsnummer",
            "en": "Service Order Number"
          },
          "width": 0.5,
          "required": false,
