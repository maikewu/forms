      },
      "messages": {
        "onLoaded": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "asset",
                    "_functionName": "GET_PRODUCT"
                  },
                  "output": {
                    "_field": "asset",
                    "_propertyName": "product"
                  }
