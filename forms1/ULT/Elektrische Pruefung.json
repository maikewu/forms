        "check4": {
          "width": 0.5,
          "fieldType": "CHECKBOX",
          "hideInPdfIfEmpty": false,
          "label": {
            "de": "Schutzleiter gegen Selbstlockern und Korrosion gesichert",
            "en": "Protective conductor secured against self-loosening and corrosion"
