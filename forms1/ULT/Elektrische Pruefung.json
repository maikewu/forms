    "02_mission_infos": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "Visual inspection",
        "de": "Sichtprüfung"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "children": {
        "check1": {
          "width": 0.5,
          "fieldType": "CHECKBOX",
          "hideInPdfIfEmpty": false,
          "label": {
            "de": "Betriebsmittel sind für die Einflüsse am Einsatzbereich geeignet",
            "en": "Operating resources are suitable for the influences at the operating site"
          }
        },
        "check2": {
          "width": 0.5,
          "fieldType": "CHECKBOX",
          "hideInPdfIfEmpty": false,
          "label": {
            "de": "Kennzeichnungen, Anschlüsse u. evtl. Trennstellen in Ordnung",
            "en": "Markings, connections and any disconnection points in order"
          }
        },
        "check3": {
          "width": 0.5,
          "fieldType": "CHECKBOX",
          "hideInPdfIfEmpty": false,
          "label": {
            "de": "Keine erkennbaren Schäden und Mängel",
            "en": "No visible damage and defects"
