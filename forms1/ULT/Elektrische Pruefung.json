          }
        },
        "check6": {
          "width": 0.5,
          "fieldType": "CHECKBOX",
          "hideInPdfIfEmpty": false,
          "label": {
            "de": "Schutz durch Isolierung aller aktiven Teile",
            "en": "Protection through insulation of all active parts"
          }
        }
      },
      "messages": {
        "onLoaded": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "tasks",
