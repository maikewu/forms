{
  "general": {
    "name": {
      "en": "Service report",
      "de": "Servicebericht"
    },
    "confirmation": {
      "isSignatureNeeded": true,
      "popupHeadline": {
        "en": "Signature",
        "de": "Unterschrift"
      },
      "popupDescription": {
        "en": "Please sign the service report.",
        "de": "Bitte unterschreiben Sie den Servicebericht."
      }
    }
  },
  "formStructure": {
    "01_generalInformation": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "General information",
        "de": "Allgemeine Daten"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "children": {
        "customer": {
          "fieldType": "REMBERG_CUSTOMER_SELECT",
          "required": false,
          "width": 1,
          "label": {
            "de": "Kunde",
            "en": "Customer"
          },
          "messages": {
            "onChange": [
              {
                "to": "01_generalInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "asset",
                        "_propertyName": "customerFilter"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": "customer",
                              "_propertyName": "customer"
                            },
                            "_functionName": "GET_ACCOUNT"
                          },
                          "_propertyName": "company"
                        },
                        "_propertyName": "billingAddress"
                      },
                      "output": {
                        "_field": "customerLocation"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "customercontactperson",
                        "_propertyName": "account"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "customer_2": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Kunde (falls nicht in der Auswahl auffindbar)",
            "en": "Customer (if not findable in the selection)"
          },
          "required": false,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": true,
          "disabled": false
        },
        "customerLocation": {
          "width": 1,
          "fieldType": "REMBERG_ADDRESS",
          "label": {
            "de": "Kundenadresse",
            "en": "Customer address"
          },
          "hideInPdfIfEmpty": true,
          "required": false,
          "messages": {
            "onChange": [
              {
                "to": "07-signatureTechnician",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customerLocation",
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature_technician",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              },
              {
                "to": "08-signatureCustomer",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customerLocation",
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature_customer",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "dosageType": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Dosieranlage",
            "en": "Dosing unit"
          },
          "required": false,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "ontopHead": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Auftragskopf",
            "en": "Application head"
          },
          "required": false,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "orderNumber": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Auftragsnummer",
            "en": "Contract number"
          },
          "required": false,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "asset": {
          "fieldType": "REMBERG_ASSET_SELECT",
          "required": true,
          "disableSelection": false,
          "hidePreselection": false,
          "showUnassigned": true,
          "editShowInput": true,
          "registerAssetChange": true,
          "hiddenInPdf": false,
          "width": 1,
          "label": {
            "de": "Maschinennummer",
            "en": "Serial number"
          }
        },
        "assetHeader": {
          "fieldType": "H3",
          "hiddenInPdf": true,
          "width": 1,
          "text": {
            "de": "Weitere Maschinen",
            "en": "Additional machines"
          }
        },
        "machine_repeater": {
          "isForm": true,
          "isListForm": true,
          "displayButtons": true,
          "listFormNoDataHint": {
            "en": "No entries added yet.",
            "de": "Es wurden noch keine Einträge hinzugefügt."
          },
          "disableTabularView": true,
          "children": {
            "asset2": {
              "fieldType": "REMBERG_ASSET_SELECT",
              "required": true,
              "disableSelection": false,
              "hidePreselection": false,
              "showUnassigned": true,
              "editShowInput": true,
              "registerAssetChange": true,
              "hiddenInPdf": false,
              "label": {
                "de": "Weitere Maschinennummer",
                "en": "Additional serial number"
              }
            }
          }
        },
        "customercontactperson": {
          "fieldType": "REMBERG_USER_SELECT",
          "label": {
            "de": "Ansprechpartner (Kunde)",
            "en": "Customer name"
          },
          "required": false,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "customerContact": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Ansprechpartner (falls nicht in der Auswahl auffindbar)",
            "en": "Customer name (if not findable in the selection)"
          },
          "width": 1,
          "required": false,
          "hideInPdfIfEmpty": true
        },
        "emptyRow23": {
          "fieldType": "H3",
          "hiddenInPdf": false,
          "width": 1,
          "text": {
            "de": " ",
            "en": " "
          }
        }
      },
      "messages": {
        "onLoaded": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": {
                      "_field": "ARGS",
                      "_propertyName": "asset",
                      "_functionName": "GET_PRODUCT"
                    },
                    "_propertyName": "customer"
                  },
                  "output": {
                    "_field": "customer",
                    "_propertyName": "customer"
                  }
                }
              }
            ]
          },
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "asset",
                    "_functionName": "GET_PRODUCT"
                  },
                  "output": {
                    "_field": "asset",
                    "_propertyName": "product"
                  }
                }
              }
            ]
          },
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "address",
                    "_functionName": "GET_ADDRESS"
                  },
                  "output": {
                    "_field": "customerLocation",
                    "_propertyName": ""
                  }
                }
              }
            ]
          },
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "customer"
                  },
                  "output": {
                    "_field": "customercontactperson",
                    "_propertyName": "account"
                  }
                }
              }
            ]
          }
        ]
      }
    },
    "01_1_reisezeiten": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "Traveling and working hours",
