  "formStructure": {
    "01_generalInformation": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "General Information",
        "de": "Allgemeine Daten"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "children": {
        "customer": {
          "fieldType": "REMBERG_CUSTOMER_SELECT",
          "required": true,
          "width": 1,
          "label": {
            "de": "Kunde",
            "en": "Customer"
          },
          "messages": {
            "onChange": [
              {
                "to": "01_generalInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "asset",
                        "_propertyName": "customerFilter"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "orderNumber": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Auftragsnummer",
            "en": "Work Order Number"
          },
          "required": true,
          "width": 1,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "asset": {
          "fieldType": "REMBERG_ASSET_SELECT",
          "required": true,
          "disableSelection": false,
          "hidePreselection": false,
          "showUnassigned": true,
          "editShowInput": true,
          "registerAssetChange": true,
          "hiddenInPdf": false,
          "width": 1,
          "label": {
            "de": "Maschinennummer",
            "en": "Machine Number"
          }
        }
      },
      "messages": {
        "onLoaded": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": {
                      "_field": "ARGS",
                      "_propertyName": "asset",
                      "_functionName": "GET_PRODUCT"
                    },
                    "_propertyName": "customer"
                  },
                  "output": {
                    "_field": "customer",
                    "_propertyName": "customer"
                  }
                }
              }
            ]
          },
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "asset",
                    "_functionName": "GET_PRODUCT"
                  },
                  "output": {
                    "_field": "asset",
                    "_propertyName": "product"
                  }
                }
              }
            ]
          }
        ]
      }
    },
