          "required": false,
          "width": 0.5,
          "label": {
            "de": "Kundenname",
            "en": "Customer"
          },
          "messages": {
            "onChange": [
              {
                "to": "01_generalInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": "customer",
                              "_propertyName": "customer"
                            },
                            "_functionName": "GET_ACCOUNT"
                          },
                          "_propertyName": "company"
                        },
                        "_propertyName": "billingAddress"
                      },
                      "output": {
                        "_field": "customerLocation"
                      }
                    }
                  }
                ]
              },
              {
                "to": "02_assetInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
