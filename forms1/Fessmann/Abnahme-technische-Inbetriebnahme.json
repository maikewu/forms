                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customerLocation",
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature2",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
