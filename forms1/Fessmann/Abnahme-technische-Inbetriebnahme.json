            "en": "Customer"
          },
          "required": false,
          "width": 1,
          "messages": {
            "onChange": [
              {
                "to": "2-customerInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "asset",
                        "_propertyName": "customerFilter"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": "customer",
                              "_propertyName": "customer"
                            },
                            "_functionName": "GET_ACCOUNT"
                          },
                          "_propertyName": "company"
                        },
                        "_propertyName": "billingAddress"
                      },
                      "output": {
                        "_field": "customerLocation"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "customerLocation": {
          "fieldType": "REMBERG_ADDRESS",
          "label": {
            "de": "Kundenadresse",
            "en": "Customer Address"
          },
          "required": false,
          "width": 1,
          "messages": {
            "onChange": [
              {
                "to": "11-signature_technician",
