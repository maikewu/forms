                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customerLocation",
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature1",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
