{
  "general": {
    "name": {
      "en": "Working Report",
      "de": "Arbeitsbericht"
    },
    "confirmation": {
      "isSignatureNeeded": true,
      "popupHeadline": {
        "en": "Sign Service Report",
        "de": "Unterschrift"
      },
      "popupDescription": {
        "en": "Please sign the working report.",
        "de": "Bitte unterschreiben Sie den Arbeitsbericht."
      }
    }
  },
  "formStructure": {
    "01_generalInformation": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "General Information",
        "de": "Allgemeine Informationen"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "children": {
        "customer": {
          "fieldType": "REMBERG_CUSTOMER_SELECT",
          "required": true,
          "width": 1,
          "label": {
            "de": "Kunde",
            "en": "Customer"
          },
          "messages": {
            "onChange": [
              {
                "to": "01_generalInformation",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "asset",
                        "_propertyName": "customerFilter"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": {
                                "_field": "customer",
                                "_propertyName": "customer"
                              },
                              "_functionName": "GET_ACCOUNT"
                            },
                            "_propertyName": "company"
                          },
                          "_propertyName": "billingAddress"
                        },
                        "_propertyName": "city"
                      },
                      "output": {
                        "_field": "customer_city"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": {
                                "_field": "customer",
                                "_propertyName": "customer"
                              },
                              "_functionName": "GET_ACCOUNT"
                            },
                            "_propertyName": "company"
                          },
                          "_propertyName": "billingAddress"
                        },
                        "_propertyName": "country"
                      },
                      "output": {
                        "_field": "customer_country"
                      }
                    }
                  },
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer",
                        "_propertyName": "customer"
                      },
                      "output": {
                        "_field": "customercontactperson",
                        "_propertyName": "account"
                      }
                    }
                  }
                ]
              },
              {
                "to": "03_signature_customer",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": {
                                "_field": "customer",
                                "_propertyName": "customer"
                              },
                              "_functionName": "GET_ACCOUNT"
                            },
                            "_propertyName": "company"
                          },
                          "_propertyName": "billingAddress"
                        },
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature1",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              },
              {
                "to": "04_signature_technician",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": {
                          "_field": {
                            "_field": {
                              "_field": {
                                "_field": "customer",
                                "_propertyName": "customer"
                              },
                              "_functionName": "GET_ACCOUNT"
                            },
                            "_propertyName": "company"
                          },
                          "_propertyName": "billingAddress"
                        },
                        "_propertyName": "city"
                      },
                      "output": {
                        "_formArray": "signature2",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "customer_city": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Montageort",
            "en": "Place"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "messages": {
            "onChange": [
              {
                "to": "03_signature_customer",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer_city"
                      },
                      "output": {
                        "_formArray": "signature1",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              },
              {
                "to": "04_signature_technician",
                "actions": [
                  {
                    "name": "PATCH_DATA",
                    "_dynamicData": {
                      "input": {
                        "_field": "customer_city"
                      },
                      "output": {
                        "_formArray": "signature2",
                        "_formArrayIndex": 0,
                        "_propertyName": "location"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        "customer_country": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Land",
            "en": "Country"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false
        },
        "asset": {
          "fieldType": "REMBERG_ASSET_SELECT",
          "required": true,
          "disableSelection": false,
          "hidePreselection": false,
          "showUnassigned": true,
          "editShowInput": true,
          "registerAssetChange": true,
          "hiddenInPdf": false,
          "width": 0.5,
          "label": {
            "de": "Maschine / Anlage",
            "en": "Machine / System"
          }
        },
        "operatingHours": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Betriebsstunden",
            "en": "Operating Hours"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "year": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Jahr",
            "en": "Year"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "week": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Woche",
            "en": "Week"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "projectNr": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Projekt-Nr.",
            "en": "Project No."
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "projektbezeichnung": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Projekt-Bezeichnung",
            "en": "Project Name"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "customercontact": {
          "fieldType": "INPUT_TEXT",
          "label": {
            "de": "Ansprechpartner Kunde",
            "en": "Contact Person Customer"
          },
          "required": false,
          "width": 0.5,
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "disabled": false
        },
        "technician": {
          "fieldType": "REMBERG_USER_SELECT",
          "label": {
            "de": "FILL Monteur",
            "en": "FILL Engineer"
          },
          "hiddenInPdf": false,
          "hideInPdfIfEmpty": false,
          "width": 0.5,
          "autofillCurrentUser": true,
          "printEmailAddress": false,
          "required": false
        }
      },
      "messages": {
        "onLoaded": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": {
                      "_field": "ARGS",
                      "_propertyName": "asset",
                      "_functionName": "GET_PRODUCT"
                    },
                    "_propertyName": "customer"
                  },
                  "output": {
                    "_field": "customer",
                    "_propertyName": "customer"
                  }
                }
              }
            ]
          },
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "PATCH_DATA",
                "_dynamicData": {
                  "input": {
                    "_field": "ARGS",
                    "_propertyName": "asset",
                    "_functionName": "GET_PRODUCT"
                  },
                  "output": {
                    "_field": "asset",
                    "_propertyName": "product"
                  }
                }
              }
            ]
          }
        ]
      }
    },
    "02_performed_tasks_customRepeater": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": true,
      "cardHeadline": {
        "en": "Work Performed",
        "de": "Ausgef체hrte Arbeiten"
      },
      "stateIndicator": true,
      "hasNextStep": true,
      "children": {
        "taskList": {
          "isForm": true,
          "isListForm": true,
          "displayButtons": true,
          "listFormNoDataHint": {
            "en": "No entries added yet.",
            "de": "Es wurden noch keine Eintr채ge hinzugef체gt."
          },
          "children": {
            "repeater_date": {
              "fieldType": "REMBERG_DATE",
              "label": {
                "de": "Datum",
                "en": "Date"
              },
              "hiddenInPdf": false,
              "autofillCurrentDate": false,
              "required": false
            },
            "start_time": {
              "fieldType": "REMBERG_TIME",
              "required": false,
              "label": {
                "de": "Von",
                "en": "From"
              },
              "hiddenInPdf": false,
              "autofillCurrentTime": false
            },
            "end_time": {
              "fieldType": "REMBERG_TIME",
              "required": false,
              "label": {
                "de": "Bis",
                "en": "Until"
              },
              "hiddenInPdf": false,
              "autofillCurrentTime": false
            },
            "tasks_repeater": {
              "fieldType": "TEXTAREA",
              "label": {
                "de": "Ausgef체hrte Arbeiten",
                "en": "Work Performed"
              },
              "hiddenInPdf": false,
              "required": false
            },
            "comments_repeater": {
              "fieldType": "TEXTAREA",
              "label": {
                "de": "Bemerkung",
                "en": "Notes"
              },
              "hiddenInPdf": false,
              "required": false
            }
          }
        }
      }
    },
    "03_signature_customer": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": false,
      "cardHeadline": {
        "en": "Signature Customer",
        "de": "Unterschrift Kunde"
      },
      "stateIndicator": true,
      "width": 0.5,
      "unbreakable": true,
      "hasNextStep": true,
      "children": {
        "signature1": {
          "fieldType": "REMBERG_SIGNATURE",
          "label": {
            "de": "Kunde",
            "en": "Customer"
          },
          "hiddenInPdf": false,
          "showTime": false,
          "autofillCurrentUser": false,
          "required": true
        }
      }
    },
    "04_signature_technician": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": false,
      "cardHeadline": {
        "en": "Signature FILL",
        "de": "Unterschrift FILL"
      },
      "stateIndicator": true,
      "width": 0.5,
      "unbreakable": true,
      "hasNextStep": true,
      "children": {
        "signature2": {
          "fieldType": "REMBERG_SIGNATURE",
          "label": {
            "de": "FILL",
            "en": "FILL"
          },
          "hiddenInPdf": false,
          "showTime": false,
          "autofillCurrentUser": true,
          "required": true
        }
      }
    },
    "05-send": {
      "isForm": true,
      "ui": "CARD",
      "initialExpand": false,
      "cardHeadline": {
        "en": "Send/sent form as E-Mail",
        "de": "Bericht als E-Mail senden/gesendet"
      },
      "stateIndicator": true,
      "hiddenInPdf": true,
      "hasNextStep": true,
      "children": {
        "send2": {
          "fieldType": "REMBERG_SEND",
          "label": {
            "de": "Gesendet an",
            "en": "Sent to"
          },
          "prefillConfig": {
            "subject": "Arbeitsbericht - FILL GmbH",
            "content": "Diese E-Mail wurde automatisch generiert.\nBitte antworten Sie nicht auf diese E-Mail.",
            "to": {
              "currentUser": false
            },
            "cc": {
              "currentUser": true
            },
            "bcc": {
              "currentUser": false
            }
          },
          "required": false
        }
      },
      "messages": {
        "onFinish": [
          {
            "to": "ROOT",
            "actions": [
              {
                "name": "SAVE_TO_BACKEND"
              }
            ]
          }
        ]
      }
    }
  }
}