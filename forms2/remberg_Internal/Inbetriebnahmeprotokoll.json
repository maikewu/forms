{
  "$schema": "https://pastebin.com/raw/19EgpgEK",
  "general": {
    "singleInstanceForm": false,
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "generalInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "General Information",
          "de": "Allgemeine Informationen"
        }
      },
      "fields": [
        {
          "id": "orderNumber",
          "type": "multiLineTextInput",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Protokollnummer",
                "en": "Report number"
              }
            }
          }
        },
        {
          "id": "acceptanceDate",
          "type": "dateInput",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Datum",
                "en": "Date"
              }
            }
          }
        },
        {
          "id": "serviceTechnicians",
          "type": "userSingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Auftraggeber",
                "en": "Contractor"
              }
            }
          }
        },
        {
          "id": "technicianSupport",
          "type": "userSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Begleitung",
                "en": "Supported by"
              }
            }
          }
        }
      ]
    },
    {
      "id": "assetInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Asset Information",
          "de": "Asset-Informationen"
        }
      },
      "fields": [
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                so far no label has been used for this input field
              }
            },
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress"
                ]
              },
              {
                "target": {
                  "id": "customer"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToCustomerAccountId",
                  "accountIdToAccount",
                  "accountToAccountInfo"
                ]
              },
              {
                "target": {
                  "id": "customerContactPerson"
                },
                "steps": [
                  insert steps here - tbd - Uwe support needed
                ]
              }
            ]
          }
        },
        {
          "id": "year",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Baujahr",
                "en": "Year of manufacture"
              }
            }
          }
        },
        {
          "id": "assetComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "3-customerInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Client Information",
          "de": "Auftraggeber"
        }
      },
      "fields": [
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Kunde",
                "en": "Customer"
              }
            }
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "required": {
              "city": false,
              "company": false,
              "country": false,
              "state": false,
              "street": false,
              "streetNumber": false,
              "supplement": false,
              "zipPostCode": false
            },
            "label": {
              "text": {
                "de": "Kundenadresse",
                "en": "Customer Address"
              }
            }
          }
        },
        {
          "id": "customerContactPerson",
          "type": "userSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Ansprechpartner-Auswahl",
                "en": "Select Customer Contact"
              }
            }
          }
        }
      ]
    },
    {
