{
  "general": {
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "1_generalInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "General Information",
          "de": "Allgemeine Informationen"
        }
      },
      "fields": [
        {
          "id": "orderNumber",
          "type": "multiLineTextInput",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Protokollnummer",
                "en": "Report number"
              }
            }
          }
        },
        {
          "id": "acceptanceDate",
          "type": "dateInput",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Datum",
                "en": "Date"
              }
            },
            "prefill": {
              "value": [
                {
                  "input": "creationDateTime",
                  "steps": [
                  "dateTimeToDate"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "serviceTechnicians",
          "type": "userSingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Auftraggeber",
                "en": "Contractor"
              }
            },
            "prefill": {
              "value": [
                {
                  "input": "assignedUserId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
          "id": "technicianSupport",
          "type": "userSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Begleitung",
                "en": "Supported by"
              }
            }
          }
        }
      ]
    },
    {
      "id": "2_assetInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Asset Information",
          "de": "Asset-Informationen"
        }
      },
      "fields": [
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Maschine",
                "en": "Asset"
              }
            },
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress"
                ]
              },
              {
                "target": {
                  "id": "customer"
                },
                "steps": [
                  "assetInfoToCustomerId",
                  "accountIdToAccount",
                  "accountToAccountInfo"
                ]
              },
              {
                "target": {
                  "id": "9_signature_technician",
                  "propertyName": "location"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress",
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "10_signature_customer",
                  "propertyName": "location"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress",
                  "addressToCityString"
                ]
              }
            ]
          }
        },
        {
          "id": "year",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Baujahr",
                "en": "Year of manufacture"
              }
            }
          }
        },
        {
          "id": "assetComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "3_customerInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Client Information",
          "de": "Auftraggeber"
        }
      },
      "fields": [
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "required": true,
            "label": {
              "text": {
                "de": "Kunde",
                "en": "Customer"
              }
            },
            "prefill": {
              "selectedCompany": [
                {
                  "input": "organizationId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "required": {
              "city": false,
              "company": false,
              "country": false,
              "state": false,
              "street": false,
              "streetNumber": false,
              "supplement": false,
              "zipPostCode": false
            },
            "label": {
              "text": {
                "de": "Kundenadresse",
                "en": "Customer Address"
              }
            },
            "prefill": {
              "value": [
                {
                  "input": "organizationId",
                  "steps": [
                    "accountIdToAccount",
                    "accountToBillingAddressAddress"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "customerContactPerson",
          "type": "userSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Ansprechpartner-Auswahl",
                "en": "Select Customer Contact"
              }
            }
          }
        }
      ]
    },
    {
      "id": "4_performedTasksInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Functional test",
          "de": "Funktionsprüfung"
        }
      },
      "fields": [
        {
          "id": "performedTests",
          "type": "taskListInput",
          "config": {
            "requiredAmountOfEntries": 1,
            "label": {
              "text": {
                "de": "Durchgeführte Tests",
                "en": "Performed Tests"
              }
            },
            "fields": {
              "checkboxBooleanInput": {
                "checkedByDefault": false,
                "pdfHide": false
              },
              "titleSingleLineTextInput": {
                "required": false,
                "pdfHide": false
              },
              "commentMultiLineTextInput": {
                "disabled": false,
                "required": false,
                "pdfHide": false
              },
              "highPrioBooleanInput": {
                "disabled": false,
                "pdfHide": false
              },
              "notNecessaryBooleanInput": {
                "disabled": false,
                "pdfHide": false
              }
            },
            "prefill": {
              "entries": [
                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToTasks"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "tasksComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "5_travelAndWorkingTimeInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Travel and Working Times",
          "de": "Reise- und Arbeitszeiten"
        }
      },
      "fields": [
        {
          "id": "tavelAndWorkingTimes",
          "type": "timeTrackingListInput",
          "config": {
            "requiredAmountOfEntries": 0,
            "label": {
              "text": {
                "de": "Zeiten",
                "en": "Times"
              }
            },
            "fields": {
              "timeTrackingTypeStaticSingleSelect": {
                "pdfHide": false
              },
              "startDateTimeInput": {
                "pdfHide": false
              },
              "endDateTimeInput": {
                "pdfHide": false
              },
              "breakNumericInput": {
                "disabled": false,
                "required": true,
                "pdfHide": false
              },
              "durationDisplay": {
                "disabled": false,
                "pdfHide": false
              },
              "distanceNumericInput": {
                "disabled": false,
                "required": true,
                "pdfHide": false
              },
              "technicianUserSingleSelect": {
                "disabled": false,
                "required": true,
                "pdfHide": false,
                "pdfPrintEmailAddress": true
              },
              "commentMultiLineTextInput": {
                "disabled": false,
                "required": true,
                "pdfHide": false
              }
            }
          }
        },
        {
          "id": "timesComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "6_expenses",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Expenses",
          "de": "Ausgaben"
        }
      },
      "fields": [
        {
          "id": "expenses",
          "type": "expenseListInput",
          "config": {
            "requiredAmountOfEntries": 0,
            "label": {
              "text": {
                "de": "Aufwände",
                "en": "Costs"
              }
            },
            "fields": {
              "dateInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "commentMultiLineTextInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "expenseAmountNumericInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "expenseTypeSingleSelectInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true,
                "values": {
                  "flight": {
                    "de": "Flug",
                    "en": "Flight"
                  },
                  "rental_car": {
                    "de": "Mietauto",
                    "en": "Rental Car"
                  },
                  "board": {
                    "de": "Verpflegung",
                    "en": "Board"
                  },
                  "accommodation": {
                    "de": "Unterkunft",
                    "en": "Accommodation"
                  },
                  "parking": {
                    "de": "Parken",
                    "en": "Parking"
                  },
                  "toll": {
                    "de": "Maut",
                    "en": "Toll"
                  },
                  "transportation": {
                    "de": "Transport",
                    "en": "Transportation"
                  },
                  "vignette": {
                    "de": "Vingette",
                    "en": "Vingette"
                  },
                  "other": {
                    "de": "Sonstiges",
                    "en": "Other"
                  }
                }
              }
            }
          }
        }
      ]
    },
    {
      "id": "7_machineStateInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Resulting Machine State",
          "de": "Resultierender Maschinenstatus"
        }
      },
      "fields": [
        {
          "id": "resultingMachineState",
          "type": "staticSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Resultierender Maschinenstatus",
                "en": "Resulting Machine State"
              }
            },
            "value": {
              "options": {
                "operational": {
                  "de": "betriebsfähig",
                  "en": "operational"
                },
                "notOperational": {
                  "de": "nicht betriebsfähig",
                  "en": "not operational"
                },
                "partlyOperational": {
                  "de": "bedingt betriebsfähig",
                  "en": "partly operational"
                }
              }
            }
          }
        },
        {
          "id": "resultingServices",
          "type": "staticSingleSelect",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Resultierende Services",
                "en": "Resulting Services"
              }
            },
            "value": {
              "options": {
                "none": {
                  "de": "keine",
                  "en": "none"
                },
                "sparePartNeeded": {
                  "de": "Ersatzteil wird benötigt",
                  "en": "spare part needed"
                },
                "additionalServiceNeeded": {
                  "de": "Zusatzservice benötigt",
                  "en": "additional service needed"
                },
                "newServiceAppointmentNeeded": {
                  "de": "Neuer Einsatz notwendig",
                  "en": "new service appointment needed"
                },
                "training": {
                  "de": "Schulung",
                  "en": "training"
                }
              }
            }
          }
        },
        {
          "id": "resultingServiceComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "8_notesInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "en": "Notes",
          "de": "Bemerkungen"
        }
      },
      "fields": [
        {
          "id": "generalNotes",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Allgemeine Bemerkungen & Hinweise",
                "en": "General Notes"
              }
            }
          }
        }
      ]
    },
    {
      "id": "9_signature_technician",
      "type": "signatureSection",
      "config": {
        "required": false,
        "hideDownloadPdfButton": false,
        "hidePreviewPdfButton": false,
        "pdfHide": false,
        "pdfHideIfValueIsEmpty": false,
        "headlineText": {
          "en": "Signature technician",
          "de": "Unterschrift Techniker"
        },
        "fields": {
          "dateInput": {
            "disable": false,
            "pdfHide": false,
            "required": true
          },
          "locationSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "required": true
          },
          "nameSingleLineTextInput": {
            "capitalizeFirstLetterOfEveryWord": true,
            "disable": false,
            "pdfHide": false,
            "required": true
          }
        },
        "prefill": {
          "location": [
            {
              "input": "workOrderId",
              "steps": [
                "workOrderIdToWorkOrder",
                "workOrderToLocationAddress",
                "addressToCityString"
              ]
            }
          ],
          "name": [
            {
              "input": "assignedUserId",
              "steps": [
                "userIdToUser",
                "userToFullNameString"
              ]
            }
          ]
        }
      }
    },
    {
      "id": "10_signature_customer",
      "type": "signatureSection",
      "config": {
        "required": false,
        "hideDownloadPdfButton": false,
        "hidePreviewPdfButton": false,
        "pdfHide": false,
        "pdfHideIfValueIsEmpty": false,
        "headlineText": {
          "en": "Signature customer",
          "de": "Unterschrift Kunde"
        },
        "fields": {
          "dateInput": {
            "disable": false,
            "pdfHide": false,
            "required": true
          },
          "locationSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "required": true
          },
          "nameSingleLineTextInput": {
            "capitalizeFirstLetterOfEveryWord": true,
            "disable": false,
            "pdfHide": false,
            "required": true
          }
        }
      }
    },
    {
      "id": "11_send",
      "type": "emailSection",
      "config": {
        "hideDownloadPdfButton": false,
        "hidePreviewPdfButton": false,
        "pdfHide": true,
        "headlineText": {
          "en": "Send filled form",
          "de": "Ausgefülltes Formular absenden"
        },
        "emailDialogPrefill": {
          "to": {
            "assignee": false,
