      },
      "fields": [
        {
          "id": "customerContactPerson",
          "type": "taskListInput",
          "config": {
            "requiredAmountOfEntries": 1,
            "label": {
              "text": {
                "de": "Durchgeführte Tests",
                "en": "Performed Tests"
              }
            }
          }
        },
        {
          "id": "tasksComment",
          "type": "multiLineTextInput",
          "config": {
            "required": false,
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
