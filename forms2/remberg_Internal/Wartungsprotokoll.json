    {
      "id": "9-Bemerkungen",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Bemerkungen",
          "en": "Notes"
        }
      },
      "fields": [
        {
          "id": "generalComments",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Allgemeine Bemerkungen & Hinweise",
