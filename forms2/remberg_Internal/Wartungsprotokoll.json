{
  "$schema": "https://pastebin.com/raw/eysKfZ7M",
  "general": {
    
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "1-allgmeineInformationen",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Allgemeine Informationen",
          "en": "General Information"
        }
      },
      "fields": [
        {
          "id": "reportID",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Wartungs-Auftragnummer",
                "en": "Maintenance Order Number"
              }
            },
            "required": true
          }
        },
        {
          "id": "orderNumber",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Bestellnummer",
                "en": "Order Number"
              }
            },
            "required": true
          }
        },
        {
          "id": "technicians",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Servicetechniker",
                "en": "Service Technician"
              }
            },
            "required": true,
            "prefill": {
              "value": [
                {
                  "input": "currentAccountId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
          "id": "begleitung",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Begleitung",
                "en": "Attendant"
              }
            }
          }
        },
        {
          "id": "ausgestelltDurch",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "ausgestellt durch",
                "en": "Issued by"
              }
            }
          }
        },
        {
          "id": "austellungsDatum",
          "type": "dateInput",
          "config": {
            "label": {
              "text": {
                "de": "Austellungsdatum",
                "en": "Date of issue"
              }
            }
          }
        }
      ]
    },
    {
      "id": "2-maschine",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Maschine",
          "en": "Machine"
        }
      },
      "fields": [
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Maschine auswählen",
                "en": "Select Asset"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress"
                ]
              },
              {
                "target": {
                  "id": "customer"
                },
                "steps": [
                  "assetInfoToCustomerId",
                  "accountIdToAccount",
                  "accountToAccountInfo"
                ]
              }
            ],
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
          "id": "constructionYear",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Baujahr",
                "en": "Construction Year"
              }
            }
          }
        }
      ]
    },
    {
      "id": "3-customerInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Kunden-Informationen",
          "en": "Customer Information"
        }
      },
      "fields": [
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Kunde",
                "en": "Customer"
              }
            },
            "required": true
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "label": {
              "text": {
                "de": "Kundenadresse",
                "en": "Customer Address"
              }
            }
          }
        },
        {
          "id": "customerContactPerson",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Ansprechpartner-Auswahl",
                "en": "Select Customer Contact"
              }
            }
          }
        }
      ]
    },
    {
