          "de": "Anlass und Ausfallursache",
          "en": "Reason and Failure Cause"
        }
      },
      "fields": [
        {
          "id": "anlass",
          "type": "staticMultiSelect",
          "config": {
            "label": {
              "text": {
                "de": "Anlass",
                "en": "Reason"
              }
            },
            "value": {
              "options": {
                "inbetriebnahme": {
                  "de": "Inbetriebnahme",
                  "en": "Commissioning"
                },
                "kundenberatung": {
                  "de": "Kundenberatung",
                  "en": "Customer advisory"
                },
                "kalibrierung": {
                  "de": "Kalibrierung",
                  "en": "Calibration"
                },
                "vorfuehrung": {
                  "de": "Vorführung",
                  "en": "Demonstration"
                },
                "endabnahme": {
                  "de": "Endabnahme",
                  "en": "Final acceptance"
                },
                "messe": {
                  "de": "Messe",
                  "en": "Fair"
                },
                "schulung": {
                  "de": "Schulung",
                  "en": "Training"
                },
                "umbau": {
                  "de": "Umbau",
                  "en": "Modification"
                },
                "nachruestung": {
                  "de": "Nachrüstung",
                  "en": "Retrofit"
                },
                "reparatur": {
                  "de": "Reparatur",
                  "en": "Repair"
                },
                "wartung": {
                  "de": "Wartung",
                  "en": "Maintenance"
                },
                "inspektion": {
                  "de": "Inspektion",
                  "en": "Inspection"
                }
              }
            }
          }
        },
        {
          "id": "ausfallUrsache",
          "type": "staticMultiSelect",
          "config": {
            "label": {
              "text": {
                "de": "Ausfallursache",
                "en": "Failure cause"
              }
            },
            "value": {
              "options": {
                "verschleiss": {
                  "de": "Verschleiß",
                  "en": "Wear"
                },
                "softwarefehler": {
                  "de": "Softwarefehler",
                  "en": "Software error"
                },
                "pc": {
                  "de": "PC",
                  "en": "PC"
                },
                "montagefehler": {
                  "de": "Montagefehler",
                  "en": "Montage error"
                },
                "transport": {
                  "de": "Transport",
                  "en": "Transport"
                },
                "bedienfehler": {
                  "de": "Bedienfehler",
                  "en": "Operating error"
                },
                "elektronik": {
                  "de": "Elektronik",
                  "en": "Electronic"
                },
                "mechanik": {
                  "de": "Mechanik",
                  "en": "Mechanics"
                },
                "verschmutzung": {
                  "de": "Verschmutzung",
                  "en": "Pollution"
                },
                "konstrfehler": {
                  "de": "Konstruktionsfehler",
                  "en": "Konstruction error"
                }
              }
            }
          }
        },
        {
          "id": "anlassUndAusfallursacheSonstiges",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "5-serviceAufgaben",
