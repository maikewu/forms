              }
            }
          }
        },
        {
          "id": "customerRequests",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Kundenwünsche",
                "en": "Customer Requests"
              }
            }
          }
        }
      ]
    },
    {
      "id": "11-signature_technician",
      "type": "signatureSection",
      "config": {
        "required": true,
        "headlineText": {
          "de": "Unterschrift Techniker",
          "en": "Signature technician"
        },
        "fields": {
          "dateInput": {
            "disable": false,
            "pdfHide": false,
            "required": false
          },
          "locationSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "required": false
          },
          "nameSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "capitalizeFirstLetterOfEveryWord": false,
            "required": false
          }
        },
        "pdfHide": false,
        "pdfHideIfValueIsEmpty": false,
        "hideDownloadPdfButton": false,
        "hidePreviewPdfButton": false
      }
    },
    {
      "id": "12-send",
      "type": "emailSection",
      "config": {
        "headlineText": {
          "de": "Ausgefülltes Formular absenden",
          "en": "Send filled form"
        },
        "hidePreviewPdfButton": false,
        "hideDownloadPdfButton": false,
        "emailDialogPrefill": {
          "bcc": {
            "assignee": false,
            "creator": false,
            "currentUser": true,
            "staticEmails": []
          },
          "cc": {
            "assignee": false,
            "creator": true,
            "currentUser": false,
            "staticEmails": []
          },
          "to": {
            "assignee": true,
            "creator": false,
            "currentUser": false,
            "staticEmails": []
          },
          "body": {
            "dynamic": "tbd"
          },
          "subject": {
            "dynamic": "tbd"
          }
        }
      }
    }
  ]
}