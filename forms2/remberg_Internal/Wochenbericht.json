{
  "$schema": "https://pastebin.com/raw/eysKfZ7M",
  "general": {
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "1-allgmeineInformationen",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Allgemeine Informationen",
          "en": "General Information"
        }
      },
      "fields": [
        {
          "id": "calenderweek",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Kalenderwoche",
                "en": "Calendar week"
              }
            }
          }
        },
        {
          "id": "technicians",
          "type": "personListInput",
          "config": {
            "label": {
              "text": {
                "de": "Servicetechniker",
                "en": "Service technicians"
              }
            }
          }
        },
        {
          "id": "ausgestelltDurch",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Ausgestellt durch",
                "en": "Issued by"
              }
            }
          }
        },
        {
          "id": "ausstelungsDatum",
          "type": "dateInput",
          "config": {
            "label": {
              "text": {
                "de": "Austellungsdatum",
                "en": "Date of Issue"
              }
            }
          }
        }
      ]
    },
    {
      "id": "2-maschine",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Maschinenlistung von dieser Woche",
