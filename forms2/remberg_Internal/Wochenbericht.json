      "fields": [
        {
          "id": "headline",
          "type": "htmlDisplay",
          "config": {
            "text": {
              "de": "<h3>Asset</h3>",
              "en": "<h3>Asset</h3>"
            }
          }
        },
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Maschine",
                "en": "Machine"
              }
            },
            "required": true
          }
        },
        {
          "id": "comment",
          "type": "multiLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Kommentar",
                "en": "Comment"
              }
            }
          }
        }
      ]
    },
    {
      "id": "3-reiseUndArbeitszeiten",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Gesamtereise- und Arbeitszeiten",
          "en": "Overall travel and Working Times"
