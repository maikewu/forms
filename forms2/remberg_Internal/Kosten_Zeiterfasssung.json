{
  "$schema": "https://pastebin.com/raw/eysKfZ7M",
  "general": {
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "01_generalInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Allgemeine Informationen",
          "en": "General Information"
        }
      },
      "fields": [
        {
          "id": "maintenanceNr",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Wartungs-Auftragnummer",
                "en": "Work Order Number"
              }
            },
            "required": true
          }
        },
        {
          "id": "orderNr",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Bestellnummer",
                "en": "Order Number"
              }
            },
            "required": true
          }
        },
        {
          "id": "technician",
          "type": "personListInput",
          "config": {
            "label": {
              "text": {
                "de": "Servicetechniker",
                "en": "Technician"
              }
            }
          }
        },
        {
          "id": "attendant",
          "type": "personListInput",
          "config": {
            "label": {
              "text": {
                "de": "Begleiter",
                "en": "Attendant"
              }
            }
          }
        },
        {
          "id": "begleitung",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Begleitung",
                "en": "Attendant"
              }
            }
          }
        },
        {
          "id": "ausgestelltDurch",
          "type": "userSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Ausgestellt durch",
                "en": "Issued by"
              }
            }
          }
        },
        {
          "id": "austellungsDatum",
          "type": "dateInput",
          "config": {
            "label": {
              "text": {
                "de": "Austellungsdatum",
                "en": "Date of issue"
              }
            }
          }
        }
      ]
    },
    {
      "id": "2_maschine",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Objekt",
          "en": "Asset"
        }
      },
      "fields": [
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Maschine auswählen",
                "en": "Select Asset"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "assetInfoToAsset",
                  "assetToLocationAddress"
                ]
              },
              {
                "target": {
                  "id": "customer"
                },
                "steps": [
                  "assetInfoToCustomerId",
                  "accountIdToAccount",
                  "accountToAccountInfo"
                ]
              }
            ],
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
          "id": "constructionYear",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "de": "Baujahr",
                "en": "Construction Year"
              }
            }
          }
        }
      ]
    },
    {
      "id": "3_reiseUndArbeitszeiten",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Reise- und Arbeitszeiten",
          "en": "Travel and Working Times"
        }
      },
      "fields": [
        {
          "id": "times",
          "type": "timeInput",
          "config": {
            "label": {
              "text": {
                "de": "Zeiten",
                "en": "Times"
              }
            }
          }
        }
      ]
    },
    {
      "id": "4_ersatzteile",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Ersatzteile",
          "en": "Spare Parts"
        }
      },
      "fields": [
        {
          "id": "parts",
          "type": "partListInput",
          "config": {
            "label": {
              "text": {
                "de": "Ersatzteile",
                "en": "Spare Parts"
              }
            }
          }
        }
      ]
    },
    {
      "id": "5_kosten",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Expenses",
          "en": "Kosten"
        }
      },
      "fields": [
        {
          "id": "expenses",
          "type": "expenseListInput",
          "config": {
            "label": {
              "text": {
                "de": "Aufwände",
                "en": "Costs"
              }
            },
            "fields": {
              "dateInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "commentMultiLineTextInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "expenseAmountNumericInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true
              },
              "expenseTypeSingleSelectInput": {
                "pdfHide": false,
                "disabled": false,
                "required": true,
                "values": {
                  "tax": {
                    "de": "Steuern",
                    "en": "Tax"
                  },
                  "travel": {
                    "de": "Reisekosten",
                    "en": "Travel Cost"
                  },
                  "parts": {
                    "de": "Materialkosten",
                    "en": "Parts Cost"
                  },
                  "labor": {
                    "de": "Arbeitskosten",
                    "en": "Labor Cost"
                  },
                  "other": {
                    "de": "Sonstiges",
                    "en": "Other"
                  }
                }
              }
            }
          }
        }
      ]
    },
    {
      "id": "06_signature_technician",
      "type": "signatureSection",
      "config": {
        "required": true,
        "headlineText": {
          "de": "Unterschrift Techniker",
          "en": "Signature technician"
        },
        "fields": {
          "dateInput": {
            "disable": false,
            "pdfHide": false,
            "required": false
          },
          "locationSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "required": false
          },
          "nameSingleLineTextInput": {
            "disable": false,
            "pdfHide": false,
            "capitalizeFirstLetterOfEveryWord": false,
            "required": false
          }
        },
        "pdfHide": false,
        "pdfHideIfValueIsEmpty": false,
        "hideDownloadPdfButton": false,
        "hidePreviewPdfButton": false
      }
    },
    {
      "id": "12_send",
      "type": "emailSection",
      "config": {
        "headlineText": {
          "de": "Ausgefülltes Formular absenden",
          "en": "Send filled form"
        },
        "hidePreviewPdfButton": false,
        "hideDownloadPdfButton": false,
        "emailDialogPrefill": {
          "bcc": {
            "assignee": false,
            "creator": false,
            "currentUser": true,
            "staticEmails": []
          },
          "cc": {
            "assignee": false,
            "creator": true,
            "currentUser": false,
            "staticEmails": []
          },
          "to": {
            "assignee": true,
            "creator": false,
            "currentUser": false,
            "staticEmails": []
          },
          "body": {
            "dynamic": "tbd"
          },
          "subject": {
            "dynamic": "tbd"
          }
        }
      }
    }
  ]
}
