{
  "$schema": "https://pastebin.com/raw/eysKfZ7M",
  "general": {
    
    "timezone": "Europe/Berlin"
  },
  "sections": [{
    "id": "generalInformation",
    "type": "fieldSection",
    "config": {
      "headlineText": {
        "en": "General Information",
        "de": "Allgemeine Informationen"
      }
    },
    "fields": [{
      "id": "orderNumber",
      "type": "singleLineTextInput",
      "config": {
        "required": true,
        "label": {
          "text": {
            "de": "Auftragsnummer",
            "en": "Order number"
          }
        }
      }
    }, {
      "id": "acceptanceDate",
      "type": "dateInput",
      "config": {
        "required": true,
        "label": {
          "text": {
            "de": "Datum",
            "en": "Date"
          }
        }
      }
    }, {
      "id": "serviceTechnicians",
      "type": "userSingleSelect",
      "config": {
        "required": true,
        "label": {
          "text": {
            "de": "Abnahme gemeldet von",
            "en": "Acceptance report ordered by"
          }
        }
      }
    }, {
      "id": "technicianSupport",
      "type": "userSingleSelect",
      "config": {
        "label": {
          "text": {
            "de": "Begleitung",
            "en": "Supported by"
          }
        }
      }
    }]
  }, {
    "id": "assetInformation",
    "type": "fieldSection",
    "config": {
      "headlineText": {
        "en": "Asset Information",
        "de": "Objekt-Informationen"
      }
    },
    "fields": [{
      "id": "asset",
      "type": "assetSingleSelect",
      "config": {
        "required": true,
        "label": {
          "text": {
            "en": "Select an asset",
            "de": "Anlage auswählen"
          }
        },
        "prefill": {
          "selectedAsset": [{
            "input": "assetId",
            "steps": []
          }]
        },
        "onChange": [{
          "target": { "id": "customerLocation" },
          "steps": [
            "assetInfoToCustomerId",
            "accountIdToAccount",
            "accountToBillingAddressAddress"
          ]
        }, {
          "target": { "id": "customer" },
          "steps": [
            "assetInfoToCustomerId",
            "accountIdToAccount",
            "accountToAccountInfo"
          ]
        }]
      }
    }, {
      "id": "year",
      "type": "singleLineTextInput",
      "config": {
        "label": {
          "text": {
            "de": "Baujahr",
            "en": "Year of manufacture"
          }
        }
      }
    }, {
      "id": "assetComment",
      "type": "singleLineTextInput",
      "config": {
        "label": {
          "text": {
            "de": "Kommentar",
            "en": "Comment"
          }
        }
      }
    }]
  }, {
    "id": "3-customerInformation",
    "type": "fieldSection",
    "config": {
      "headlineText": {
        "en": "Customer Information",
        "de": "Kunden-Informationen"
      }
    },
    "fields": [{
      "id": "customer",
      "type": "companySingleSelect",
      "config": {
        "required": true,
        "label": {
          "text": {
            "de": "Kunde",
            "en": "Customer"
          }
        }
      }
    }, {
      "id": "customerLocation",
      "type": "addressInput",
      "config": {
        "label": {
          "text": {
            "de": "Kundenadresse",
            "en": "Customer Address"
          }
        }
      }
    }, {
      "id": "customerContactPerson",
      "type": "userSingleSelect",
      "config": {
        "label": {
          "text": {
            "de": "Ansprechpartner-Auswahl",
            "en": "Select Customer Contact"
          }
        }
      }
    }]
  }, {
    "id": "performedTasksInformation",
    "type": "fieldSection",
    "config": {
      "headlineText": {
        "en": "Performed Tasks",
        "de": "Durchgeführte Arbeiten"
      }
    },
    "fields": [{
      "id": "tasks",
      "type": "taskListInput",
      "config": {
        "label": {
          "text": {
            "de": "Durchgeführte Arbeiten",
            "en": "Performed Tasks"
          }
        },
        "requiredAmountOfEntries": 1
      }
    }, {
      "id": "tasksComment",
      "type": "singleLineTextInput",
      "config": {
        "label": {
          "text": {
            "de": "Kommentar",
            "en": "Comment"
          }
        }
      }
    }]
