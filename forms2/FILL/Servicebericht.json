          "type": "companySingleSelect",
          "config": {
            "required": true,
            "pdfWidth": 1,
            "label": {
              "text": {
                "de": "Kunde",
                "en": "Customer"
              }
            },
            "useAsFilterForFields": ["asset","customerContactPerson"],
            "prefill": {
              "selectedCompany": [
                {
                  "input": "organizationId",
                  "steps": []
                },
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    "assetToCustomerAccountId"
                  ]
                }
              ]
            },
            "onChange": [
              {
                "target": {"id": "customerLocation"},
                "steps": ["accountInfoToAccount", "accountToBillingAddressAddress"]
              }
