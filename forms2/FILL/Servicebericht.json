          "id": "reportID",
          "config": {
            "label": {
              "text": {
                "de": "Service-Auftragsnummer",
                "en": "Service order number"
              }
            },
            "pdfHideIfValueIsEmpty": true,
            "required": true,
            "pdfWidth": 0.5
          }
        },
        {
          "type": "personListInput",
          "id": "technicians",
          "config": {
            "label": {
              "text": {
                "de": "Techniker",
                "en": "Technician"
              }
            },
            "pdfHideIfValueIsEmpty": true,
            "pdfWidth": 0.5,
            "requiredAmountOfEntries": 1,
