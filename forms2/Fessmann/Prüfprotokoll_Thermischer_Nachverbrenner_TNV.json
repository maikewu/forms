                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToOrganizationAccountId"
                  ]
                },
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    "assetToCustomerAccountId"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "pdfWidth": 0.5,
            "label": {
              "text": {
                "en": "Location of the equipment:",
                "de": "Standort der Anlage"
              }
            },
            "pdfHideIfValueIsEmpty": true,
            "onChange": [
              {
                "target": {
                  "id": "A1-signatureTechnican",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "A1-signatureCustomer",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              }
