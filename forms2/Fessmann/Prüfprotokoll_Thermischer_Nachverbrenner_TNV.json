              "text": {
                "en": "Name of Customer",
                "de": "Name des Betreibers"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "accountInfoToAccount",
                  "accountToBillingAddressAddress"
                ]
              }
            ],
            "useAsFilterForFields": [
