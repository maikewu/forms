            "label": {
              "text": {
                "en": "Serialnumber",
                "de": "Seriennummer"
              }
            },
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            }
          }
        },
        {
