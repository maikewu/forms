                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "B1-signatureTechnican",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "A1-signatureCustomer",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "B1-signatureCustomer",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              }
            ],
            "prefill": {
              "value": [
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    "assetToLocationAddress"
                  ]
                },
                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToLocationAddress"
                  ]
                }
              ]
