    {
      "id": "01-generalInformation",
      "type": "fieldSection",
      "config": {
        "headline": {
          "text": {
            "en": "General Information",
            "de": "Allgemeine Informationen"
          },
          "pdfHide": true
        }
      },
      "fields": [
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "pdfHideIfValueIsEmpty": false,
            "pdfWidth": 0.5,
            "label": {
              "text": {
                "en": "Name of Customer",
                "de": "Name des Betreibers"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "accountInfoToAccount",
                  "accountToBillingAddressAddress"
                ]
              }
            ],
            "useAsFilterForFields": ["asset"],
            "prefill": {
              "selectedCompany": [
                {
                  "input": "organizationId",
                  "steps": []
                },
                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToOrganizationAccountId"
                  ]
                },
                {
                  "input": "assetId",
                  "steps": ["assetIdToAsset", "assetToCustomerAccountId"]
                }
              ]
            }
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "pdfWidth": 0.5,
            "label": {
              "text": {
                "en": "Location of the equipment",
                "de": "Standort der Anlage"
              }
            },
            "pdfHideIfValueIsEmpty": true,
            "onChange": [
              {
                "target": {
                  "id": "A1-signatureTechnican",
                  "propertyName": "location"
                },
                "steps": ["addressToCityString"]
              },
              {
                "target": {
                  "id": "A1-signatureCustomer",
                  "propertyName": "location"
                },
                "steps": ["addressToCityString"]
              }
            ],
            "prefill": {
              "value": [
                {
                  "input": "assetId",
                  "steps": ["assetIdToAsset", "assetToLocationAddress"]
                },
                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToLocationAddress"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "assetCustomer",
          "type": "singleLineTextInput",
          "config": {
            "label": {
              "text": {
                "en": "Fabrication- and serial number",
                "de": "Kundenanlagen-Bezeichnung"
              }
            },
            "pdfHideIfValueIsEmpty": true,
            "pdfWidth": 0.5,
            "prefill": {
              "value": [
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    ["assetToCustomPropertyValue", 27],
                    "customPropertyValueToString"
                  ]
                }
              ]
            }
          }
        },
        {
          "id": "asset",
