                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToOrganizationAccountId"
                  ]
                },
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    "assetToCustomerAccountId"
                  ]
