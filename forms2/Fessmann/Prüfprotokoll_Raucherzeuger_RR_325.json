          "config": {
            "label": {
              "text": {
                "de": "Kundenanlagen-Bezeichnung"
              }
            },
            "pdfHideIfValueIsEmpty": true
          }
        },
        {
          "id": "asset",
          "type": "assetSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Serialnummer"
              }
            },
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            }
