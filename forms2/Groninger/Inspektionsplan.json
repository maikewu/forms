            "pdfHide": false,
            "pdfHideIfValueIsEmpty": false,
            "pdfWidth": 1,
            "label": {
              "text": {
                "en": "The machine is ready for operation according to the current status.",
                "de": "Die Maschine ist nach aktuellem Stand betriebsbereit."
              },
              "uiHide": false,
