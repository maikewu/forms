          "id": "exampleHeadlineDisplay_2",
          "type": "headlineDisplay",
          "config": {
            "text": {
              "en": "1.1 Requirements for maintenance",
              "de": "1.1 Voraussetzungen für eine Wartung"
            },
            "uiHide": false,
            "type": "H3",
            "pdfHide": false,
            "pdfWidth": 1
