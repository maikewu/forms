    {
      "id": "01_general_information",
      "type": "fieldSection",
      "config": {
        "headline": {
          "pdfHide": true,
          "text": {
            "de": "Allgemeine Informationen",
            "en": "General Information"
          }
        }
      },
      "fields": [
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "required": false,
            "pdfHideIfValueIsEmpty": false,
            "label": {
              "text": {
                "de": "Kunde / Betreiber",
                "en": "Customer"
              },
              "pdfHide": false
            },
            "prefill": {
              "selectedCompany": [
                {
                  "input": "organizationId",
                  "steps": []
                }
              ]
            },
            "pdfHide": false,
            "onChange": [
              {
                "target": {
                  "id": "customer_location"
                },
                "steps": [
                  "accountInfoToAccount",
                  "accountToBillingAddressAddress"
                ]
              }
            ]
          }
        },
        {
          "id": "customer_location",
          "type": "addressInput",
          "config": {
            "pdfWidth": 0.5,
            "pdfHideIfValueIsEmpty": true,
            "label": {
              "text": {
                "en": "Location",
                "de": "Standort"
              }
            },
            "prefill": {
              "value": [
                {
                  "input": "workOrderId",
                  "steps": [
                    "workOrderIdToWorkOrder",
                    "workOrderToLocationAddress"
                  ]
                },
                {
                  "input": "assetId",
                  "steps": [
                    "assetIdToAsset",
                    "assetToLocationAddress"
                  ]
                },
                {
                  "input": "organizationId",
                  "steps": [
                    "accountIdToAccount",
                    "accountToBillingAddressAddress"
                  ]
                }
              ]
            },
            "onChange": [
              {
                "target": {
                  "id": "05_signature_AMI",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "06_signature_Customer",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              }
            ]
          }
        },
        {
          "id": "Anlagen",
          "type": "assetSingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Maschinenbezeichnung und Seriennummer"
              }
            },
            "prefill": {
              "selectedAsset": [
                {
                  "input": "assetId",
                  "steps": []
                }
              ]
            },
            "pdfHide": true
          }
        },
        {
          "id": "Date",
          "type": "dateInput",
          "config": {
            "label": {
              "text": {
                "de": "Datum der Wartung"
              }
            },
            "value": {
              "autofill": "always"
            }
          }
