{
  "$schema": "https://pastebin.com/raw/eysKfZ7M",
  "general": {
    "timezone": "Europe/Berlin"
  },
  "sections": [
    {
      "id": "01_customerInformation",
      "type": "fieldSection",
      "config": {
        "headlineText": {
          "de": "Kundeninformationen",
          "en": "Customer Information"
        }
      },
      "fields": [
        {
          "id": "headlineGeneralInformation",
          "type": "headlineDisplay",
          "config":{
            "uiHide": true,
            "text":{
              "de":"Kundeninformationen",
              "en":"Custonmer Information"
            },
            "pdfWidth": 1,
            "pdfTextSize": "H2"
          }
        },
        {
          "id": "customer",
          "type": "companySingleSelect",
          "config": {
            "label": {
              "text": {
                "de": "Kunde",
                "en": "Customer"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "customerLocation"
                },
                "steps": [
                  "accountInfoToAccount",
                  "accountToBillingAddressAddress"
                ]
              },
              {
                "target": {
                  "id": "phone"
                },
                "steps": [
                  "accountInfoToAccount",
                  "accountToPhoneNumberString",
                  "phoneNumberStringToPhoneNumberObject"
                ]
              }
            ],
            "prefill": {
              "selectedCompany": [
                {
                  "input": "currentAccountId",
                  "steps": []
                }
              ]
            },
            "useAsFilterForFields": [
              "asset"
            ]
          }
        },
        {
          "id": "customerLocation",
          "type": "addressInput",
          "config": {
            "label": {
              "text": {
                "de": "Kundenadresse",
                "en": "Customer Adress"
              }
            },
            "onChange": [
              {
                "target": {
                  "id": "06_signature",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              },
              {
                "target": {
                  "id": "07_signatureSWOlching",
                  "propertyName": "location"
                },
                "steps": [
                  "addressToCityString"
                ]
              }
            ]
          }
        },
        {
          "id": "phone",
          "type": "phoneNumberInput",
          "config": {
            "label": {
              "text": {
                "de": "Telefon",
                "en": "Phone"
              }
            }
          }
        }
      ]
    },
